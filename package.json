{
  "name": "@secretwpn/react-image-zoom",
  "version": "0.3.2",
  "private": false,
  "description": "Enables zooming and panning an image, both mobile and desktop.",
  "main": "dist/PinchZoomPan.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --host 0.0.0.0",
    "test": "jest",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "keywords": [
    "react",
    "reactjs",
    "responsive",
    "pinch",
    "zoom",
    "pan",
    "image"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.4.3",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "file-loader": "6.2.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.4.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "author": "Brad Stiff",
  "contributors": [
    {
      "name": "secretwpn"
    }
  ],
  "homepage": "https://github.com/secretwpn/react-responsive-pinch-zoom-pan",
  "repository": {
    "type": "git",
    "url": "git@github.com:secretwpn/react-responsive-pinch-zoom-pan.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "reselect": "^4.1.7",
    "warning": "^4.0.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
